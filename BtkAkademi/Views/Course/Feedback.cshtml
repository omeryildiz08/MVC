@model Candidate

<div>
    <h3 class="display-4">Congrats @Model.FullName</h3>
    <p class="lead">Your application <mark>received.</mark></p>
    <p class="lead">Thanks for your interest</p>
    <hr />
    <blockquote class="blockquote">
        You've enrolled to @Model.SelectedCourse course.
    </blockquote>
    <p class="text-muted">You applied at @Model.ApplyAt</p>

    <div class="btn-group">
        <a class="btn btn-outline-success" href="#">Applications</a>
        <a class="btn btn-success" asp-action="Index" asp-controller="Home">Home</a>

    </div>
</div>