@model List<StoreApp.Models.Product>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
@foreach(StoreApp.Models.Product prd in Model)
{
    <div>
        <h1>@prd.ProductName</h1>
        <p>@prd.Price</p>
    </div>
}
</body>

</html>